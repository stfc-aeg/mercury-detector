{
    "Main": {
        "receiver": {
            "process": true,
            "command": "${INSTALL_PREFIX}\/bin\/frameReceiver",
            "pos-args": "",
            "sleep": 2
        },
        "processor": {
            "process": true,
            "command": "${INSTALL_PREFIX}\/bin\/frameProcessor",
            "pos-args": "",
            "sleep": 2
        },
        "simulator": {
            "process": false,
            "command": "${INSTALL_PREFIX}\/bin\/frameSimulator",
            "pos-args": "Mercury",
            "sleep": 1
        },
        "test": {
            "process": false,
            "command": "${INSTALL_PREFIX}\/bin\/frameTests --log_level=all -- ",
            "pos-args": "",
            "sleep": 1
        }
    },
    "simulator":
    {
        "lib-path": "${INSTALL_PREFIX}\/lib",
        "frames": "10",
        "dest-ip": "127.0.0.1",
        "ports": "61649",
        "pattern-path": "${INSTALL_PREFIX}\/test_config\/patterns\/CSD_average.json",
        "interval": "1"
    },
    "receiver":
    {
        "json_file": "${INSTALL_PREFIX}\/test_config\/mercury-fr.json",
        "m": "500000000"
    },
    "processor":
    {
        "json_file": "${INSTALL_PREFIX}\/test_config\/mercury-fp-average.json"
    },
    "test":
    {
        "json": "${INSTALL_PREFIX}\/test_config\/test-average.json"
    }
}
